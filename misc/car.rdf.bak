<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.pizza.com/ontologies/pizza.owl#"
     xml:base="http://www.pizza.com/ontologies/pizza.owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.pizza.com/ontologies/pizza.owl">
        <rdfs:comment>A car ontology for everyday cars :-)</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasBody -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasBody">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isBodyOf"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Car"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Body"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasChassis -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasChassis">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isChassisOf"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Car"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Chassis"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasEngineCapacity -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasEngineCapacity">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Car"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#EngineCapacityValuePartition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasHorsePower -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasHorsePower">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Car"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#HorsePowerValuePartition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#isBodyOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#isBodyOf">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Body"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Car"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#isChassisOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#isChassisOf">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Chassis"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Car"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasEngineCapacityValue -->

    <owl:DatatypeProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasEngineCapacityValue"/>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasHorsePowerValue -->

    <owl:DatatypeProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasHorsePowerValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza.com/ontologies/pizza.owl#BackBoneChassis -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#BackBoneChassis">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Chassis"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#LadderChassis"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#Body -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#Body"/>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#Car -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#Car">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasChassis"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Chassis"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasHorsePowerValue"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#Chassis -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#Chassis"/>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#CompactSedan -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#CompactSedan">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Body"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasHorsePower"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Medium"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#Convertible -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#Convertible">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Body"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasHorsePower"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#StreetLegal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#DragRacingCar -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#DragRacingCar">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#RacingCar"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasBody"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#CompactSedan"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Hatchback"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Sedan"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozzarella and Tomato</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#EngineCapacityValuePartition -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#EngineCapacityValuePartition">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#ValuePartition"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#FamilyCar -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#FamilyCar">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Car"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasEngineCapacityValue"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2500</xsd:maxInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Car"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#FormulaF1Car -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#FormulaF1Car">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#RacingCar"/>
        <rdfs:comment>A pizza that only has Mozzarella and Tomato</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#Hatchback -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#Hatchback">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Body"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasHorsePower"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#NotStreetLegal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#HighCapacity -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#HighCapacity">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#EngineCapacityValuePartition"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#HorsePowerValuePartition -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#HorsePowerValuePartition">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Medium"/>
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#NotStreetLegal"/>
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#StreetLegal"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#ValuePartition"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#LadderChassis -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#LadderChassis">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Chassis"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#Limousine -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#Limousine">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Body"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasHorsePower"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#NotStreetLegal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#LowCapacity -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#LowCapacity">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#EngineCapacityValuePartition"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#LuxuryCar -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#LuxuryCar">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasBody"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Limousine"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Car"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#Medium -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#Medium">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#HorsePowerValuePartition"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#MediumCapacity -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#MediumCapacity">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#EngineCapacityValuePartition"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#NotStreetLegal -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#NotStreetLegal">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#HorsePowerValuePartition"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#RacingCar -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#RacingCar">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Car"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#RallyCar -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#RallyCar">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#RacingCar"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasBody"/>
                <owl:allValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Sedan"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozzarella and Tomato</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#Sedan -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#Sedan">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Body"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasHorsePower"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Medium"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#SohoPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#SohoPizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#RacingCar"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasBody"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Hatchback"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Limousine"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozzarella and Tomato</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#SpaceChassis -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#SpaceChassis">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Chassis"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#SpicyPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#SpicyPizza">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasBody"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Body"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasHorsePower"/>
                                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#StreetLegal"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Car"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#StreetLegal -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#StreetLegal">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#HorsePowerValuePartition"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#TwoSeaterCar -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#TwoSeaterCar">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Car"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasHorsePowerValue"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Car"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#ValuePartition -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#ValuePartition"/>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#VegetarianPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#VegetarianPizza">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasBody"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Hatchback"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Limousine"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Car"/>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza.com/ontologies/pizza.owl#ExampleFormula1Car -->

    <owl:NamedIndividual rdf:about="http://www.pizza.com/ontologies/pizza.owl#ExampleFormula1Car">
        <rdf:type rdf:resource="http://www.pizza.com/ontologies/pizza.owl#FormulaF1Car"/>
        <hasHorsePowerValue rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">600</hasHorsePowerValue>
    </owl:NamedIndividual>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#ExampleRallyCar -->

    <owl:NamedIndividual rdf:about="http://www.pizza.com/ontologies/pizza.owl#ExampleRallyCar">
        <rdf:type rdf:resource="http://www.pizza.com/ontologies/pizza.owl#RallyCar"/>
        <hasHorsePowerValue rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">363</hasHorsePowerValue>
    </owl:NamedIndividual>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // General axioms
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#CompactSedan"/>
